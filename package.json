{
    "name": "pocket",
    "version": "0.2.23",
    "description": "Pouch DB model library",
    "main": "dist/src/index.js",
    "types": "dist/src/index.d.ts",
    "private": true,
    "scripts": {
        "build": "tsc --project tsconfig.json && tsc-alias -p tsconfig.json && rm -Rf dist/mocks && rm -Rf dist/spec && npm pack",
        "lint": "eslint src/**/*.ts",
        "repl": "ts-node -r tsconfig-paths/register",
        "test": "ts-node -r tsconfig-paths/register node_modules/jasmine/bin/jasmine --config=jasmine.json",
        "coverage": "nyc -r text -e .ts -x \"tests/*.test.ts\" npm run test"
    },
    "postinstall": "patch-package",
    "keywords": [
        "Pouch DB",
        "Model",
        "Database",
        "MVC"
    ],
    "author": "Kent Ng",
    "license": "ISC",
    "devDependencies": {
        "@types/jasmine": "^4.3.1",
        "@types/pluralize": "0.0.29",
        "@types/pouchdb": "^6.4.0",
        "@typescript-eslint/eslint-plugin": "^5.51.0",
        "@typescript-eslint/parser": "^5.51.0",
        "eslint": "^8.33.0",
        "eslint-config-airbnb-base": "^15.0.0",
        "eslint-config-prettier": "^8.6.0",
        "eslint-config-standard-with-typescript": "^34.0.0",
        "eslint-plugin-import": "^2.27.5",
        "eslint-plugin-n": "^15.6.1",
        "eslint-plugin-promise": "^6.1.1",
        "jasmine": "^4.5.0",
        "msw": "^1.1.0",
        "nyc": "^15.1.0",
        "patch-package": "^6.5.1",
        "ts-node": "^10.9.1",
        "tsc-alias": "^1.1.1",
        "tsconfig-paths": "^4.1.2",
        "typescript": "^4.9.5"
    },
    "dependencies": {
        "axios": "^1.3.4",
        "comdb": "^6.0.0-beta",
        "events": "^3.3.0",
        "moment": "^2.29.1",
        "pluralize": "^8.0.0",
        "pouchdb": "^8.0.1",
        "pouchdb-adapter-memory": "^8.0.1",
        "pouchdb-browser": "^8.0.1",
        "pouchdb-find": "^8.0.0",
        "short-uuid": "^4.2.2"
    },
    "engines": {
        "node": ">=19.0.0"
    }
}
